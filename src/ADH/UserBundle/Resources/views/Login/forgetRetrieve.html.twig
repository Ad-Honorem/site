{% extends "::base.html.twig" %}

{% block content %}
	<article style="margin: auto; max-width: 660px;">
		{{ form_start(form, {"action": path("adh_user_token_check", {"user": token.user.username, "type": token.type.type, "token": token.token})}) }}
			{{ form_errors(form) }}
			
			{# Password #}
			{{ form_row(form.password) }}
			
			{# Submit #}
			{{ form_row(form.submit) }}
		{{ form_end(form) }}
	</article>
{% endblock content %}